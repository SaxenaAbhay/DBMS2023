
package com.dbms.sms.entity;

//import java.io.Serializable;

import jakarta.persistence.*;

@Entity
@Table(name="scores")
public class Score {
	
	@EmbeddedId scoreId scoreId; 
	
	@Column(name="marks")
	private Long marks;
}
class scoreId{
	
	@ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "exam_id", referencedColumnName = "exam_id")
	private Exam examId;
	

	@ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "scholar_id", referencedColumnName = "scholar_id")
	private Student scholorId;
}
