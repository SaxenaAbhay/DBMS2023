
package com.dbms.sms.entity;

//import java.io.Serializable;

import jakarta.persistence.*;

@Entity
@Table(name="scores")
public class Score {
	
	@EmbeddedId scoreId scoreId; 
	
	@Column(name="marks")
	private Long marks;
}
class scoreId{
	
	@Column(name="exam_id")
	private int examId;
	
	@Column(name="scholor_id")
	private int scholorId;
}
