
package com.dbms.sms.entity;

//import java.io.Serializable;

import jakarta.persistence.*;

@Entity
@Table(name="scores")
public class Score {
	
	@EmbeddedId
	@Column(name="studId")
	private int studId;
	
	@EmbeddedId
	@Column(name="subjId")
	private int subjId;
	
	@Column(name="score")
	private Long score;
}
